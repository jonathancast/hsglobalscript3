#! /usr/local/plan9/bin/rc

cd $HSGLOBALSCRIPT/src/hsgs2hs &&
    ./integration-test &&
    ./install-all &&
cd ../gsi && # implementation implementation
    ./integration-test &&
cd ../gsio && # very simple file I/O
cd ../hs2gs && # Re-write hsghs2hs in gsio, and add "gs2hs" for compiling full Global Script programs
cd ../gscat && # write a string to stdout
cd ../gshtml && # HTML templating system
true

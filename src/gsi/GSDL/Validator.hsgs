module GSDL.Validator (gsvalidator_env_in_scope, gsvalidator_env_in_scope_insert, gsvalidator_env_empty) where

$gsimports

gsvalidator_env_in_scope = [gs:value| λ 'v. λ 'env. ordered-set.∈ gsvar.<=> v (env #in-scope) |]


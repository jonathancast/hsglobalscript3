#! /usr/local/plan9/bin/rc

. $GS_SPEC/bin/test-fns
. $GS_SPEC/bin/gs-spec

GLOBALSCRIPT=$HSGLOBALSCRIPT

fn test_gsi_impl { ./gsio $* }

test_non_existent_file
ok_grep_errors '-- gsi program --' 'gsi-prog.gsio.gs:1:1: Incorrect type: gsi.m is not gsio.m' ./gsio gsi-prog.gsio.gs
test_empty_file

true
